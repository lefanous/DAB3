@model IEnumerable<DAB3_Assignment.Models.Circle>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Users)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.ActionLink(item.Name, "Wall", new { name = item.Name })
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Users.Count)
                </td>
            </tr>
        }
    </tbody>
</table>
